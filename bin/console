#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

use Symfony\Component\Console\Application;

$app = new Application('TYPO3 Surf', '0.0.1');
$app->add(new \TYPO3\Surf\Command\ListCommand());
$app->add(new \TYPO3\Surf\Command\SimulateCommand());
$app->add(new \TYPO3\Surf\Command\DescribeCommand());
$app->add(new \TYPO3\Surf\Command\DeployCommand());
$app->run();