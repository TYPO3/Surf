{
    "name": "typo3/surf",
    "description": "TYPO3 Surf is a deployment tool, suited for a wide variety of applications",
    "license": "GPL-3.0+",
    "require": {
        "symfony/console": "^2.8",
        "symfony/process": "^2.8",
        "ircmaxell/random-lib": "^1.1",
        "psr/log": "^1.0",
        "monolog/monolog": "^1.17"
    },
    "require-dev": {
        "symfony/finder": "^2.8",
        "phpunit/phpunit": "^4.8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "TYPO3\\Surf\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TYPO3\\Surf\\Tests\\": "Tests"
        }
    },
    "bin": ["bin/surf"]
}
