{
    "name": "typo3/surf",
    "description": "TYPO3 Surf is a deployment tool, suited for a wide variety of applications",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": ">=7.0.8",
        "monolog/monolog": "^1.17",
        "neos/utility-files": "^3.0",
        "padraic/phar-updater": "1.0.5",
        "symfony/console": "^3.4.17 || ^4.0",
        "symfony/process": "^3.0 || ^4.0",
        "symfony/options-resolver": "^3.0 || ^4.0",
        "guzzlehttp/guzzle": "^6.0",
        "webmozart/assert": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "symfony/finder": "^3.0 || ^4.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "sphpdox/sphpdox": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "TYPO3\\Surf\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "TYPO3\\Surf\\Tests\\": "tests"
        }
    },
    "bin": [
        "surf"
    ],
    "config": {
        "platform": {
            "php": "7.0.33"
        }
    }
}
