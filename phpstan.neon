includes:
    - phpstan-baseline.neon

parameters:
    level: 8
    parallel:
        # to prevent full thread lagging pc
        maximumNumberOfProcesses: 7
    editorUrl: 'phpstorm://open?file=%%file%%&line=%%line%%'
    paths:
        - src
        - tests

    inferPrivatePropertyTypeFromConstructor: true
    treatPhpDocTypesAsCertain: false
